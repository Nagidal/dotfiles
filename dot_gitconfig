[pull]
	ff = only
[lfs "https://dotfiles.github.com/Nagidal/dotfiles.git/info/lfs"]
	locksverify = true
[lfs "https://chazre.github.com/Nagidal/chazre.git/info/lfs"]
	locksverify = true
[lfs "https://bootstrap.github.com/Nagidal/bootstrap.git/info/lfs"]
	locksverify = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
